{
  "name": "@sux/chapter-map-component",
  "version": "0.10.0",
  "description": "React component for showing chapter distribution for Game Workers Unite",
  "author": {
    "name": "Game Workers Unite",
    "url": "https://gameworkersunite.org/"
  },
  "homepage": "https://github.com/gameworkers/chapter-map-component",
  "repository": {
    "type": "git",
    "url": "https://github.com/gameworkers/chapter-map-component.git"
  },
  "bugs": {
    "url": "https://github.com/gameworkers/chapter-map-component/issues"
  },
  "license": "AGPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "tooltip-styles.css"
  ],
  "scripts": {
    "dev": "parcel example/index.html -d tmp/build",
    "build": "run-s -l build:*",
    "build:clean": "rm -rf dist",
    "build:cjs": "tsc -p tsconfig.build.cjs.json",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:docs": "parcel build example/index.html -d docs",
    "test": "run-s -l test:*",
    "test:eslint": "eslint '{src,test}/**/*.{js,ts,tsx}'",
    "test:prettier": "prettier '{src,test}/**/*.{js,ts,tsx}' -l",
    "test:ts": "tsc --noEmit",
    "prettier": "prettier --write '{src,test}/**/*.{js,ts,tsx}'",
    "prepublishOnly": "run-s build"
  },
  "dependencies": {
    "@sux/react-simple-maps": "^2.1.4",
    "react-popper-tooltip": "^3.1.0",
    "swr": "^0.2.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-simple-maps": "^1.0.3",
    "eslint": "^7.4.0",
    "eslint-config-lostfictions": "^0.1.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "react": "^16.8.4"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  }
}
