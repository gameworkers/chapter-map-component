{
  "name": "@gameworkers/chapter-map-component",
  "version": "0.8.0",
  "description": "React component for showing chapter distribution for Game Workers Unite",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": ["dist"],
  "scripts": {
    "dev": "parcel example/index.html -d tmp/build",
    "build": "run-s -l build:*",
    "build:clean": "rm -rf dist",
    "build:cjs": "tsc -p tsconfig.build.cjs.json",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:example": "parcel build example/index.html -d docs",
    "prepublishOnly": "run-s build",
    "test": "run-s -l test:*",
    "test:eslint": "eslint '{src,test}/**/*.{js,ts,tsx}'",
    "test:prettier": "prettier '{src,test}/**/*.{css,js,ts,tsx}' -l",
    "test:ts": "tsc --noEmit",
    "prettier": "prettier --write '{src,test}/**/*.{css,js,ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gameworkers/chapter-map-component.git"
  },
  "engines": {
    "node": ">=10"
  },
  "author": {
    "name": "Game Workers Unite",
    "url": "https://gameworkersunite.org/"
  },
  "license": "AGPL-3.0",
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "dependencies": {
    "@gameworkers/react-simple-maps": "^0.12.1-pan-fix",
    "react-popper-tooltip": "^3.0.0",
    "react-simple-maps": "^2.1.2",
    "swr": "^0.2.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-simple-maps": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "typescript": "^3.9.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
